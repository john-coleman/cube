<h1>Listing ad_computer_accounts</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th>Machine account</th>
      <th>Organizational unit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ad_computer_accounts.each do |ad_computer_account| %>
      <tr>
        <td><%= ad_computer_account.domain %></td>
        <td><%= ad_computer_account.machine_account %></td>
        <td><%= ad_computer_account.ou %></td>
        <td><%= link_to 'Show', ad_computer_account %></td>
        <td><%= link_to 'Edit', edit_ad_computer_account_path(ad_computer_account) %></td>
        <td><%= link_to 'Destroy', ad_computer_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New AD computer account', new_ad_computer_account_path %>
